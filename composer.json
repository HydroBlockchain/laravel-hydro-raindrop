{
    "name": "adrenth/laravel-hydro-raindrop",
    "description": "Integrates Hydro Raindrop to your Laravel application.",
    "keywords": ["laravel", "hydro", "raindrop", "2fa", "mfa"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alwin Drenth",
            "email": "adrenth@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "adrenth/raindrop-sdk": "^1.2"
    },
    "require-dev": {
        "laravel/framework": "5.8.*"
    },
    "autoload": {
        "psr-4": {
            "Adrenth\\LaravelHydroRaindrop\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adrenth\\LaravelHydroRaindrop\\ServiceProvider"
            ]
        }
    }
}
